    "@types/yup": "^0.28.0",
    "awesome-phonenumber": "^2.47.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "i18next": "^19.9.2",
    "moment": "^2.29.1",
    "node-sass": "^4.13.1",
    "react": "^17.0.1",
    "react-avatar": "^3.10.0",
    "react-bootstrap": "^1.5.1",
    "react-dom": "^17.0.1",
    "react-facebook-login": "^4.1.1",
